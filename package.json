{
  "name": "yanlinlin.cn",
  "version": "1.0.0",
  "description": "**http://yanlinlin.cn/**",
  "main": "index.js",
  "scripts": {
    "copy-fonts": "mkdir -p static/webfonts && cp node_modules/@fortawesome/fontawesome-free/webfonts/*.woff2 static/webfonts/",
    "build:css": "npm run copy-fonts && sass src/scss/main.scss static/css/main.css --style compressed --quiet-deps",
    "build:js": "webpack",
    "build": "npm run build:css && npm run build:js",
    "dev:css": "npm run copy-fonts && sass src/scss/main.scss static/css/main.css --watch --quiet-deps",
    "dev:js": "webpack --watch",
    "dev": "npm run dev:css & npm run dev:js",
    "hugo:fast": "npm run build && hugo --config hugo-fast.yaml",
    "hugo:full": "npm run build && hugo",
    "hugo:fast:server": "npm run build && hugo server --config hugo-fast.yaml --port 13131",
    "hugo:full:server": "npm run build && hugo server --port 13131",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanlinlin82/yanlinlin.cn.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yanlinlin82/yanlinlin.cn/issues"
  },
  "homepage": "https://github.com/yanlinlin82/yanlinlin.cn#readme",
  "devDependencies": {
    "sass": "^1.89.2",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.7",
    "fuse.js": "^7.1.0"
  }
}
